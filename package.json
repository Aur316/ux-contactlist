    "axios": "^1.7.4",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.6",
    "formidable": "^3.5.1",
    "framer-motion": "^11.3.29",
    "joi": "^17.13.3",
    "lru-cache": "^11.0.0",
    "multer": "^1.4.5-lts.1",
    "next": "14.2.6",
    "next-connect": "^1.0.0",
    "next-rate-limit": "^0.0.3",
